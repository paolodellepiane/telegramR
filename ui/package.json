{
  "name": "telegramr",
  "version": "1.0.0",
  "description": "a lightweight telegram gui",
  "main": "src/index.ts",
  "repository": "https://github.com/pd29/telegramR.git",
  "author": "pd",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist ../src/d.sz",
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --public-url ./",
    "inline": "inline-assets dist/index.html ../src/d",
    "compress": "szip ../src/d",
    "cargo": "cd .. && cargo build --release",
    "release": "run-s clean build inline compress",
    "releaseall": "run-s release cargo"
  },
  "dependencies": {
    "@hyperapp/logger": "^0.5.0",
    "@hyperapp/transitions": "^1.0.2",
    "@types/node": "^10.3.3",
    "assets": "^3.0.0",
    "config.json": "^0.0.4",
    "core-js": "^2.5.7",
    "dist": "^0.1.2",
    "hyperapp": "^1.2.6",
    "hyperapp-devtools": "^0.3.0",
    "hyperapp-redux-devtools": "^1.1.6",
    "lodash": "^4.17.11",
    "rxjs": "^6.2.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "inline-assets": "^1.2.7",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.8.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
